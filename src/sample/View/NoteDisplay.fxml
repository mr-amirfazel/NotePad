<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="noteDisplay" prefHeight="450.0" prefWidth="740.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.NoteDisplayController">
   <children>
      <Label fx:id="titlelabel" alignment="CENTER" layoutX="41.0" prefHeight="84.0" prefWidth="618.0" text="Label">
         <font>
            <Font name="Andalus" size="45.0" />
         </font>
      </Label>
      <Label fx:id="noteTextLabel" alignment="TOP_LEFT" layoutX="47.0" layoutY="131.0" prefHeight="311.0" prefWidth="618.0" text="Label">
         <font>
            <Font size="32.0" />
         </font>
      </Label>
      <VBox layoutX="668.0" prefHeight="450.0" prefWidth="73.0">
         <children>
            <ImageView fitHeight="71.0" fitWidth="70.0" onMouseClicked="#goToNoteBook" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Images/goBack.png" />
               </image>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </ImageView>
            <ImageView fx:id="starState" fitHeight="61.0" fitWidth="71.0" onMouseClicked="#starUnStar" pickOnBounds="true" preserveRatio="true" />
            <CheckBox fx:id="checkState" mnemonicParsing="false" onAction="#checkNote" prefHeight="55.0" prefWidth="72.0">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets left="29.0" />
               </padding>
            </CheckBox>
            <ImageView fitHeight="73.0" fitWidth="69.0" onMouseClicked="#deleteNote" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Images/del.png" />
               </image>
            </ImageView>
         </children>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></VBox>
   </children>
</AnchorPane>
