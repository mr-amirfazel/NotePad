<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="settingPane" prefHeight="450.0" prefWidth="747.0" stylesheets="@Styles/Setting.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.Setting">
   <children>
      <GridPane layoutX="175.0" layoutY="51.0" prefHeight="322.0" prefWidth="389.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="160.59998168945313" minHeight="10.0" prefHeight="160.59998168945313" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="163.8000244140625" minHeight="10.0" prefHeight="162.20001831054688" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="removeNotes" mnemonicParsing="false" onAction="#removeAllNotes" prefHeight="159.0" prefWidth="198.0" text="Remove All Notes">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button fx:id="returnBtn" mnemonicParsing="false" onAction="#returnToMenu" prefHeight="159.0" prefWidth="198.0" text="Go Back" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <font>
                  <Font name="System Bold Italic" size="31.0" />
               </font>
            </Button>
            <Button fx:id="myInfoButton" mnemonicParsing="false" onAction="#userInfo" prefHeight="159.0" prefWidth="198.0" text="MY Info" GridPane.rowIndex="1">
               <font>
                  <Font name="System Bold Italic" size="34.0" />
               </font>
            </Button>
            <Button fx:id="profilePic" mnemonicParsing="false" onAction="#alterProfilePic" prefHeight="159.0" prefWidth="198.0" text="Profile Pic" GridPane.columnIndex="1">
               <font>
                  <Font name="System Bold Italic" size="27.0" />
               </font>
            </Button>
         </children>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </GridPane>
   </children>
</AnchorPane>
